{
  "name": "@borkdominik-biguml/model-langium-generator",
  "version": "3.0.0",
  "description": "Yeoman generator to setup Langium with Model Service - enabling graphical editors access to the AST",
  "keywords": [
    "yeoman-generator",
    "language",
    "dsl",
    "lsp",
    "vscode"
  ],
  "license": "MIT",
  "author": "David JÃ¤ger",
  "bin": {
    "generator-langium-model-management": "./generators/cli.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "compile": "tsc -b",
    "lint": "eslint -c ./eslint.config.js",
    "lint:fix": "eslint --fix -c ./eslint.config.js",
    "watch": "tsc -w",
    "debug": "npx --node-arg=--inspect yo langium",
    "run": "yo langium"
  },
  "dependencies": {
    "@borkdominik-biguml/big-outline": "*",
    "@borkdominik-biguml/big-property-palette": "*",
    "chalk": "4.1.2",
    "fast-xml-parser": "^4.3.5",
    "lodash": "~4.17.21",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.2.2",
    "typescript-graph": "^0.3.0",
    "prettier": "^3.5.1",
    "yeoman-generator": "^7.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "@types/lodash": "^4.14.195",
    "@types/reflect-metadata": "^0.1.0",
    "@types/which": "~3.0.0",
    "@yeoman/types": "^1.1.1",
    "ts-morph": "^26.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
