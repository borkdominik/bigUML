{
  "name": "@borkdominik-biguml/model-server",
  "displayName": "model-server",
  "version": "0.0.1",
  "description": "Please enter a brief description here",
  "categories": [
    "Programming Languages"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/src/vscode/index.d.ts",
      "default": "./lib/src/vscode/index.js"
    },
    "./integration": {
      "types": "./lib/gen/integration.d.ts",
      "default": "./lib/gen/integration.js"
    },
    "./grammar": {
      "types": "./lib/gen/grammar.d.ts",
      "default": "./lib/gen/grammar.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rimraf lib dist node_modules/.tmp",
    "compile": "tsc -b",
    "generate": "npm run language:generate && npm run langium:generate",
    "langium:generate": "langium generate",
    "langium:watch": "langium generate --watch",
    "language:generate": "uml-language-tooling generate -d @borkdominik-biguml/uml-language/definition -g ./gen",
    "lint": "eslint -c ./eslint.config.js",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc",
    "vscode:prepublish": "npm run build && npm run lint",
    "watch": "concurrently --kill-others 'npm run compile -- --watch'"
  },
  "contributes": {},
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.1.0",
    "@borkdominik-biguml/uml-language-tooling": "*",
    "chalk": "~4.1.2",
    "commander": "~10.0.0",
    "fast-json-patch": "3.1.1",
    "jsonpath": "1.1.1",
    "langium": "^2.1.3",
    "net": "1.0.2",
    "reflect-metadata": "^0.1.13",
    "uuid": "^9.0.1",
    "vscode-jsonrpc": "8.1.0",
    "vscode-languageclient": "9.0.1",
    "vscode-languageserver": "9.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.0.2"
  },
  "devDependencies": {
    "langium-cli": "^2.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "vscode": "^1.67.0"
  }
}
